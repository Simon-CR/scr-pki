{
  "backend": {
    "postgresql": {
      "connection_url": "postgres://{{ .Env.POSTGRES_USER }}:{{ .Env.POSTGRES_PASSWORD }}@postgres:5432/{{ .Env.POSTGRES_DB }}?sslmode=disable",
      "table": "vault_kv_store"
    }
  },
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable": 1
    }
  },
  "ui": true,
  "disable_mlock": true
}
